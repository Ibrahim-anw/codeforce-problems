#include <bits/stdc++.h>
using namespace std;
int main(){
    int n,k; cin>>n>>k;
    string s; cin>>s;
    map<char,int> m;
    for(char c:s) m[c]++;
    for(auto x:m) if(x.second%k!=0){cout<<"-1"; return 0;}
    string t;
    for(auto x:m) t+=string(x.second/k,x.first);
    for(int i=0;i<k;i++) cout<<t;
}


